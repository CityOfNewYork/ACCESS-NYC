{
  "name": "nyco/wp-assets",
  "version": "1.4.0",
  "type": "wordpress-muplugin",
  "license": "GPL-3.0-or-later",
  "description": "A developer plugin with helpers for managing assets in WordPress.",
  "homepage": "https://github.com/cityofnewyork/nyco-wp-assets",
  "authors": [
    {
      "name": "NYC Opportunity",
      "email": "products@nycopportunity.nyc.gov",
      "homepage": "https://nyc.gov/opportunity"
    }
  ],
  "keywords": [
    "nycopportunity",
    "wordpress",
    "assets",
    "configuration",
    "integrations",
    "scripts",
    "javascript",
    "styles",
    "css",
    "inline",
    "theme",
    "register",
    "enqueue",
    "query-monitor",
    "querymonitor"
  ],
  "support": {
    "issues": "https://github.com/cityofnewyork/nyco-wp-assets/issues"
  },
  "require": {
    "composer/installers": "~1.0",
    "mustangostang/spyc": "~0.6"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "~3.0"
  },
  "scripts": {
    "lint": [
      "vendor/bin/phpcs --standard=standard.xml --extensions=php"
    ],
    "fix": [
      "vendor/bin/phpcbf --standard=standard.xml --extensions=php"
    ],
    "publish": [
      "git push && git push --tags"
    ]
  }
}
