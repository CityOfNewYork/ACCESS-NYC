{
  "name": "nyco/anyc-field-screener",
  "version": "1.0.2",
  "type": "wordpress-muplugin",
  "license": "GPL-3.0+",
  "description": "The ACCESS NYC Field Screener is a tablet optimized version of the ACCESS NYC benefits eligibility screener created for outreach caseworkers.",
  "homepage": "https://github.com/cityofnewyork/nyco-anyc-field-screener",
  "authors": [
    {
      "name": "NYC Opportunity",
      "email": "products@nycopportunity.nyc.gov",
      "homepage": "https://nyc.gov/opportunity"
    }
  ],
  "keywords": [
    "wordpress",
    "field-screener",
    "eligibility",
    "screener"
  ],
  "support": {
    "issues": "https://github.com/cityofnewyork/nyco-anyc-field-screener/issues"
  },
  "require": {
    "composer/installers": "~1.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "~3.0"
  },
  "scripts": {
    "lint": [
      "vendor/bin/phpcs --standard=phpcs.xml --extensions=php --ignore=vendor/ ."
    ],
    "fix": [
      "vendor/bin/phpcbf --standard=phpcs.xml --extensions=php --ignore=vendor/ ."
    ],
    "publish": [
      "git push && git push --tags"
    ]
  }
}
