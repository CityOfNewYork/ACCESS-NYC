{
  "name": "nyco/wp-assets",
  "version": "0.0.5",
  "type": "package",
  "license": "GPL-3.0+",
  "description": "WordPress package for asset helpers.",
  "homepage": "https://github.com/cityofnewyork/nyco-wp-assets",
  "authors": [
    {
      "name": "Devon Hirth",
      "email": "dehirth@nycopportunity.nyc.gov",
      "homepage": "https://github.com/devowhippit"
    }
  ],
  "keywords": [
    "wordpress",
    "assets"
  ],
  "support": {
    "issues": "https://github.com/cityofnewyork/nyco-wp-assets/issues"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "3.*",
    "codeception/codeception": "^2.3"
  },
  "scripts": {
    "lint": [
      "vendor/bin/phpcs --standard=standard.xml --extensions=php --ignore=vendor/ ./dist"
    ],
    "format": [
      "vendor/bin/phpcbf --standard=standard.xml --extensions=php --ignore=vendor/ ./dist"
    ]
  }
}
