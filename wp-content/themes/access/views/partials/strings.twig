{% apply spaceless %}
<script type="text/javascript" nonce="{{ csp_script_nonce }}">
  window.LOCALIZED_STRINGS = window.LOCALIZED_STRINGS || [];
  window.LOCALIZED_STRINGS = window.LOCALIZED_STRINGS.concat([
    {% for s in strings %}{
      "label": {% if translation_id %}{{ __(s.label, translation_id)|trim|json_encode() }}{% else %}{{ s.label|trim|json_encode() }}{% endif %},
      "slug": {{ s.slug|trim|json_encode() }}
    }{% if not loop.last %},{% endif %}{% endfor %}
  ]);
</script>
{% endapply %}