{% for m in header_menu.get_items %}
    <a href="{{ m.link }}"
        {% if this.style %}class="{{ this.style }}"{% endif %}
        {% if m.attr_title %}title="{{ m.attr_title }}"{% else %}title="{{ m.title }}"{% endif %}
        {% if m.target %}target="{{ m.target }}"{% endif %}>
        {{ m.title }}
    </a>
{% endfor %}