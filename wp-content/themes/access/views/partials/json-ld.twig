  <script type="application/ld+json">
    {
      "@context": "https://schema.org",
      "@type": "GovernmentService",
      "name": "{{ post.program_name }}",
      "alternateName": "{{ post.plain_language_program_name }}",
      "datePosted": "{{ post.post_modified }}",
      "expires": "{{ post.custom.program_status_clear_date }}",
      "url": "{{ post.get_permalink }}",
      "serviceType": "{{ post.category.name }}",
      "serviceOperator": {
        "@type": "GovernmentOrganization",
        "name": "{{ post.government_agency }}"
      },
      "availableChannel": {
        "@type": "ServiceChannel",
        "description": "{{ post.get_field(accordion) }}"
      },
      "spatialCoverage": {
          "type": "City",
          "name": "New York"
      },
      "description": "{{ post.get_field('program_description') }}",
      "disambiguatingDescription": "{{ post.get_field('field_58912c1a8a81b')|add_anyc_checklist|add_anyc_table_numeric }}"
    },
    {
      "@context": "https://schema.org",
      "@type": "SpecialAnnouncement",
      "name": "{{ post.program_name }}",
      "category": "https://www.wikidata.org/wiki/Q81068910",
      "governmentBenefitsInfo": {
        "@type": "GovernmentService",
        "name": "{{ post.program_name }}",
        "url": "{{ post.structured_data_url }}",
        "provider": {
          "@type": "GovernmentOrganization",
          "name": "{{ post.government_agency }}"
        },
        "audience": {
          "@type": "Audience",
          "name": "{{ post.audience }}"
        },
        "serviceType": "{{ post.category.name }}"
      },
      "serviceOperator": {
        "@type": "GovernmentOrganization",
        "name": "{{ post.government_agency }}"
      },
      "spatialCoverage":
        {
          "type": "City",
          "name": "New York"
        }
    },
    {
      "@context": "https://schema.org",
      "@type": "GovernmentOffice",
      "name": "{{ post.title }}",
      "hasMap": "{{__('Get directions', 'accessnyc-locations')}}",
      "description": "{{ post.get_help }}",
      "address": {
        "@type": "PostalAddress",
        "streetAddress": "{% if post.address_street_2 %}{{ post.address_street_2}}{% endif %}",
        "addressLocality": "{{ post.city }}",
        "postalCode": "{{ post.zip }}"
      },
      "telephone": "{{ phone.number }}{% if phone.extension %}{{__('Ext.','accessnyc-locations')}} {{ phone.extension }}{% endif %}",
      "sameAs": "{{ post.website }}",
      "spatialCoverage": {
          "type": "City",
          "name": "New York"
      }
    },
    {
      "@context": "https://schema.org",
      "@type": "Organization",
      "name": "{{ post.title }}",
      "hasMap": "{{__('Get directions', 'accessnyc-locations')}}",
      "description": "{{ post.get_help }}",
      "address": {
        "@type": "PostalAddress",
        "streetAddress": "{% if post.address_street_2 %}{{ post.address_street_2}}{% endif %}",
        "addressLocality": "{{ post.city }}",
        "postalCode": "{{ post.zip }}"
      },
      "telephone": "{{ phone.number }}{% if phone.extension %}{{__('Ext.','accessnyc-locations')}} {{ phone.extension }}{% endif %}",
      "sameAs": "{{ post.website }}",
      "spatialCoverage": {
          "type": "City",
          "name": "New York"
      }
    },
    {
      "@context": "http://schema.org",
      "@type": "WebPage",
      "mainEntityOfPage": {
        "name": "{{ post.title }}",
        "dateModified": "{{ post.post_modified }}"
      }
    }
  </script>
