{
  "name": "ACCESS-NYC",
  "version": "3.3.0",
  "description": "Find help in NYC with food, money, housing, work and more on ACCESS NYC",
  "homepage": "https://access.nyc.gov",
  "authors": [
    {
      "name": "NYC Opportunity",
      "email": "products@nycopportunity.nyc.gov",
      "homepage": "http://nyc.gov/opportunity"
    },
    {
      "name": "Blue State Digital",
      "homepage": "https://www.bluestatedigital.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "filp/whoops": "^2.3",
    "whoops/soap": "^1.0",
    "wpackagist-plugin/acf-to-rest-api": "3.1.0",
    "wpackagist-plugin/better-search-replace": "1.3.2",
    "wpackagist-plugin/enable-media-replace": "3.2.1",
    "wpackagist-plugin/gathercontent-import": "3.1.9",
    "wpackagist-plugin/limit-login-attempts-reloaded": "^2.7",
    "wpackagist-plugin/timber-library": "^1.8",
    "wpackagist-plugin/search-everything": "8.1.9",
    "wpackagist-plugin/wps-hide-login": "^1.5",
    "wpackagist-plugin/wp-security-questions": "1.0.3",
    "wpackagist-plugin/rollbar": "^2.4",
    "wpackagist-plugin/bulk-delete": "5.6.1",
    "nyco/wp-assets": "0.0.5",
    "nyco/wp-config": "^0.1.0",
    "nyco/wp-open-data-transients": "^0.0.4",
    "mustangostang/spyc": "^0.6.2",
    "rarst/wps": "^1.2"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.3"
  },
  "scripts": {
    "lint": [
      "vendor/bin/phpcs --standard=phpcs.xml"
    ],
    "fix": [
      "vendor/bin/phpcbf --standard=phpcs.xml"
    ]
  }
}
